@using LAB1_FINAL.Models
@model IEnumerable<PlayerModel>

@{
    ViewBag.Title = "C_CSV";
}

<h2>CSV upload for Custom List</h2>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CSV</title>
</head>
<body>
    <div class="form-horizontal">
        @using (Html.BeginForm("CSV", "C_Player", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <input type="file" name="postedFile" />
            <input type="submit" name="Import" />
        }

    </div>
</body>

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

</html>